{
  "name": "@salesforce/command",
  "version": "0.2.5",
  "description": "SFDX base command class",
  "main": "dist/exported",
  "license": "BSD-3-Clause",
  "scripts": {
    "typings": "./node_modules/.bin/tsc -p typings/tsconfig.json",
    "lint": "./node_modules/.bin/tslint -p .",
    "lint-report": "npm run lint -- -t checkstyle -o checkstyle.xml",
    "clean": "node scripts/clean.js",
    "compile": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "build": "npm run clean && npm run lint && npm run compile",
    "watch": "./node_modules/.bin/tsc -w -p tsconfig.json",
    "test": "./node_modules/.bin/mocha --require source-map-support/register dist/test/unit/**/*Test.js",
    "test-with-coverage": "npm run lint-report && npm run compile && scripts/unit-test-with-coverage.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-command"
  ],
  "files": [
    "dist",
    "messages",
    "!dist/test/**/*",
    "dist/test/index.js",
    "!dist/**/*.map"
  ],
  "dependencies": {
    "@oclif/command": "^1.5.0",
    "@oclif/errors": "^1.2.0",
    "@oclif/parser": "^3.6.0",
    "@oclif/plugin-help": "^2.1.0",
    "@oclif/test": "^1.2.0",
    "@salesforce/core": "0.21.1",
    "@salesforce/kit": "0.0.8",
    "@salesforce/ts-types": "0.5.0",
    "chalk": "^2.4.0",
    "cli-ux": "^4.8.0"
  },
  "devDependencies": {
    "@oclif/config": "1.7.0",
    "@salesforce/dev-config": "1.1.4",
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.42",
    "@types/node": "8.0.25",
    "chai": "4.1.1",
    "mocha": "5.0.1",
    "nyc": "^12.0.2",
    "shelljs": "0.8.1",
    "sinon": "3.2.1",
    "source-map-support": "^0.5.3",
    "typescript": "3.0.1",
    "xunit-file": "1.0.0"
  },
  "types": "dist/exported.d.ts"
}
